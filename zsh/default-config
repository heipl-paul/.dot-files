current_dir=$(dirname "$0")
parent_dir=$(dirname "$current_dir")

source "$current_dir/.zsh-alias.sh"
source "$current_dir/.prompt.sh"
source "$current_dir/.zsh-git-info.sh"
source "$current_dir/.git-prompt.sh"
source "$current_dir/.zsh-utils.sh"
source "$current_dir/.gh-completion.sh"
source "$parent_dir/.alias.sh"
source "$parent_dir/.utils.sh"